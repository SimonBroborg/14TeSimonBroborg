import java.awt.*;
import java.awt.event.*;
import javax.swing.*;
import java.util.Scanner;
import java.util.HashMap;

public class Main extends JPanel{
	static Scanner sc = new Scanner(System.in);
	
	public static void main(String[] args){
		Vehicles car = new Car(100, 1584, "red");
		Vehicles bike = new Bicycle();
		Vehicles truck = new Truck();
		
		HashMap<Vehicles, String> vehicles = new HashMap();
		vehicles.put(car, "car");
		vehicles.put(bike, "bike");
		vehicles.put(truck, "truck");
		
		while(true){
			String j = sc.nextLine();
			while(j != ""){
			for(int i = 0; i < vehicles.size(); i++){

				
				
				if(j == vehicles.get(i) + " color")
					System.out.println(vehicles.get(i, i).getColor());
					j = "";
				}
				
				}
			}
			
		}
	
	
}
